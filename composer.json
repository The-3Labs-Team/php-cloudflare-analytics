{
    "name": "the-3labs-team/php-cloudflare-analytics",
    "description": "A php package to retrieve page views and other data from Cloudflare Analytics",
    "keywords": [
        "The3LabsTeam",
        "php-cloudflare-analytics"
    ],
    "homepage": "https://github.com/the-3labs-team/php-cloudflare-analytics",
    "license": "MIT",
    "authors": [
        {
            "name": "The-3Labs-Team",
            "email": "dev@3labs.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "pestphp/pest": "^2.20",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "The3LabsTeam\\PhpCloudflareAnalytics\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "The3LabsTeam\\PhpCloudflareAnalytics\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
